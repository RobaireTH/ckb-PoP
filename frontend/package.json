{
  "name": "ckb-pop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build",
    "build:vercel": "node -e \"const fs=require('fs');const keys=Object.keys(process.env).filter(k=>k.startsWith('VITE_'));if(keys.length){fs.writeFileSync('.env',keys.map(k=>k+'='+process.env[k]).join('\\n'));}\" && ng build",
    "preview": "ng serve --configuration=production",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@ckb-ccc/ccc": "^1.1.25",
    "@google/genai": "^1.40.0",
    "@vercel/analytics": "^1.6.1",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "rxjs": "^7.8.2",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/qrcode": "^1.5.6",
    "jsdom": "^28.0.0",
    "typescript": "~5.9.0",
    "vite": "^6.2.0",
    "vitest": "^4.0.18"
  }
}
